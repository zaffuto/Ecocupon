{
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "/webhook/process-order"
      },
      "name": "HTTP Trigger",
      "type": "n8n-nodes-base.httpRequest",
      "position": [250, 250]
    },
    {
      "parameters": {
        "functionCode": "const orderData = $json; return { orderData };"
      },
      "name": "Function Node - Process Order",
      "type": "n8n-nodes-base.function",
      "position": [450, 250]
    },
    {
      "parameters": {
        "operation": "update",
        "orderId": "={{$node[\"Function Node - Process Order\"].json[\"orderData\"]['orderId']}}",
        "status": "fulfilled"
      },
      "name": "Shopify Node - Update Order",
      "type": "n8n-nodes-base.shopify",
      "position": [650, 250]
    },
    {
      "parameters": {
        "responseMode": "all",
        "data": "={{$node[\"Shopify Node - Update Order\"].json}}"
      },
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respond",
      "position": [850, 250]
    }
  ],
  "connections": {
    "HTTP Trigger": {
      "main": [
        [
          {
            "node": "Function Node - Process Order",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Function Node - Process Order": {
      "main": [
        [
          {
            "node": "Shopify Node - Update Order",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Shopify Node - Update Order": {
      "main": [
        [
          {
            "node": "Respond to Webhook",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
